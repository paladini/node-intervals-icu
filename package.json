{
  "name": "intervals-icu",
  "version": "1.1.0",
  "description": "Lightweight TypeScript client library for Intervals.icu API",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "intervals.icu",
    "intervals",
    "training",
    "cycling",
    "running",
    "api",
    "client",
    "typescript"
  ],
  "author": "Fernando Paladini <fnpaladini+nodeintervals@gmail.com> (https://github.com/paladini)",
  "contributors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paladini/node-intervals-icu.git"
  },
  "bugs": {
    "url": "https://github.com/paladini/node-intervals-icu/issues"
  },
  "homepage": "https://github.com/paladini/node-intervals-icu#readme",
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
